{
    "breadcrumbs.enabled": true,
    "breadcrumbs.symbolSortOrder": "name",
    "city-lights-icons-vsc.hidesExplorerArrows": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.autoIndent": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "line",
    "editor.cursorWidth": 1,
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "'Noto Mono','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 13,
    "editor.fontWeight": "100",
    "editor.insertSpaces": true,
    "editor.lineNumbers": "interval",
    "editor.minimap.enabled": true,
    "editor.mouseWheelScrollSensitivity": 5,
    "editor.mouseWheelZoom": true,
    "editor.multiCursorModifier": "alt",
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        100
    ],
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",<>/?",
    "editor.wordWrap": "on",
    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.enableSmartCommit": true,
    "open-in-vim.openMethod": "integrated-terminal",
    "plantuml.previewFileType": "svg",
    "plantuml.exportFormat": "svg",
    "plantuml.exportOutDir": ".",
    "plantuml.previewSnapIndicators": true,
    "plantuml.server": "https://www.plantuml.com/plantuml.disabled",
    "search.showLineNumbers": true,
    "search.usePCRE2": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.env.linux": {},
    "terminal.integrated.experimentalBufferImpl": "TypedArray",
    "terminal.integrated.splitCwd": "inherited",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Badwolf Paradox",
    "workbench.startupEditor": "newUntitledFile"
}
