_notes(){
	local word=${COMP_WORDS[$COMP_CWORD-1]}
	local cur=${COMP_WORDS[COMP_CWORD]}
	case $word in

		n) local files=$(ls -1 /home/sjas/txt/ )
		   COMPREPLY=( $(compgen -W "${files}" -- $cur ) )
		;;
		cn) local files=$(ls -1 /home/sjas/txt/ )
		   COMPREPLY=( $(compgen -W "${files}" -- $cur ) )
		;;
	esac
}
complete -F _notes n cn
